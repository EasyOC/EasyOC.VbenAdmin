    "echarts": "^5.2.2",
    "intro.js": "^4.3.0",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^6.2.0",
    "pinia": "2.0.9",
    "print-js": "^1.6.0",
    "qrcode": "^1.5.0",
    "showdown": "^1.9.1",
    "sortablejs": "^1.14.0",
    "tinymce": "^5.10.2",
