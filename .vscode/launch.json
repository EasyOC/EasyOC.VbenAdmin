{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "vscode-edge-devtools.debug",
      "request": "attach",
      "name": "Attach to Microsoft Edge and open the Edge DevTools",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/src",
      "sourceMaps": true
    }
  ]
}
