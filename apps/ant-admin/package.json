{
  "name": "@apps/antd-admin",
  "version": "3.0.0-alpha.4",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vbenjs/vue-vben-admin.git",
    "directory": "apps/ant-app"
  },
  "author": {
    "name": "vben",
    "email": "anncwb@126.com",
    "url": "https://github.com/anncwb"
  },
  "scripts": {
    "build": "pnpm cross-env NODE_ENV=production vite build && pnpm esno ./scripts/post-build.ts",
    "build:test": "pnpm cross-env vite build --mode test &&pnpm esno ./scripts/post-build.ts",
    "clean": "pnpm rimraf node_modules && pnpm rimraf dist && pnpm rimraf .turbo",
    "dev": "vite",
    "gen:icon": "pnpm esno ./scripts/generate-icon.ts",
    "preview": "npm run build && vite preview",
    "preview:dist": "vite preview",
    "report": "pnpm cross-env REPORT=true npm run build",
    "server:br": "pnpx http-server dist --cors --brotli -c-1",
    "server:gzip": "pnpx http-server dist --cors --gzip -c-1",
    "type:check": "vue-tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@ant-design/colors": "^6.0.0",
    "@ant-design/icons-vue": "^6.1.0",
    "@components/antd": "workspace:*",
    "@components/common": "workspace:*",
    "@iconify/iconify": "^2.2.0",
    "@pkg/apis": "workspace:*",
    "@pkg/directives": "workspace:*",
    "@pkg/locale": "workspace:*",
    "@pkg/request": "workspace:*",
    "@pkg/setting": "workspace:*",
    "@pkg/tokens": "workspace:*",
    "@pkg/use": "workspace:*",
    "@pkg/utils": "workspace:*",
    "ant-design-vue": "3.1.0-rc.4",
    "cropperjs": "^1.5.12",
    "echarts": "^5.3.1",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^6.2.0",
    "pinia": "2.0.12",
    "sortablejs": "^1.15.0",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@config/css-preprocess": "workspace:*",
    "@config/vite": "workspace:*",
    "@iconify/json": "^2.1.20",
    "@pkg/types": "workspace:*",
    "@purge-icons/generated": "^0.8.1",
    "@types/codemirror": "^5.60.5",
    "@types/inquirer": "^8.2.0",
    "@types/mockjs": "^1.0.6",
    "@types/nprogress": "^0.2.0",
    "@types/sortablejs": "^1.10.7",
    "@vue/compiler-sfc": "3.2.31",
    "@vue/test-utils": "^2.0.0-rc.18",
    "dotenv": "^16.0.0",
    "picocolors": "^1.0.0",
    "vite": "2.9.0-beta.4",
    "vite-plugin-theme": "^0.8.6",
    "vue-tsc": "^0.33.9"
  }
}
